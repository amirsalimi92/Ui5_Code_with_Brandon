{
  "_version": "1.1.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "myapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDesc}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {}
  },
  "sap.ui": {
    "_version": "1.1.0",
    "icons": {
      "favIcon": "favicon.ico"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_bluecrystal"]
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "resources": {
      "js": [
        {
          "uri": "styles/index.js"
        }
      ],
      "css": [
        {
          "uri": "styles/style.css"
        }
      ]
    },
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "myapp.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        },
        "uri": "i18n/i18n.properties"
      }
    },
    "rootView": {
      "viewName": "myapp.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "handleValidation": false,
    "config": {},
    "routing": {},
    "extends": {},
    "contentDensities": {
      "compact": false,
      "cozy": false
    }
  },

  "sap.platform.abap": {
    "_version": "1.1.0",
    "uri": "<uri>"
  },
  "sap.platform.hcp": {
    "_version": "1.1.0",
    "uri": "<uri>"
  }
}
